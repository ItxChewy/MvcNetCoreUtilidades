@model List<Coche>

<div class="dropdown">
    <button class="btn btn-secondary dropdown-toggle" type="button" data-bs-toggle="dropdown" aria-expanded="false">
        Dropdown button
    </button>
    <ul class="dropdown-menu">
        @foreach(Coche coche in Model){
            <li><a class="dropdown-item" asp-controller="Coches"
            asp-action="Details"
            asp-route-idcoche="@coche.IdCoche">@coche.Marca</a></li>
        }
    </ul>
</div>